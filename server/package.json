{
  "name": "ck-server",
  "version": "0.0.0",
  "private": true,
  "node-main": "./bin/www",
  "main": "http://localhost:3000",
  "node-remote": "<all_urls>",
  "permissions": [
    "desktopCapture",
    "chrome-extension://*"
