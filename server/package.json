    "ignore": [
      "public/*"
    ]
  },
  "dependencies": {
    "async": "^2.5.0",
    "axios": "^0.16.2",
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "lodash": "^4.17.4",
    "mongodb": "^2.2.33",
    "mongoose": "^4.11.12",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "~1.8.1",
    "pug": "~2.0.0-beta11",
    "serve-favicon": "~2.4.2",
    "shelljs": "^0.7.8",
    "webworker-threads": "^0.7.13"
  },
  "devDependencies": {
    "nw-dev": "^3.0.1"
